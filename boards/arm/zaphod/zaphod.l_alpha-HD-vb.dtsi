                   │  W  X  M  G  J  │ │ #$  .: /* "[ '] │
                   │  C  S  N  T  K  | | ,;   A  E  I  H │
                   │  P  F  L  D  V  │ │ -+   U  O  Y  B │
                   ╰───────╮  BSP R  │ │ SPC RET  ╭──────╯
                           ╰─────────╯ ╰──────────╯   */

                l_alpha {
                        label = "l_alpha";
                        bindings = <

//   &none &none &none &none &none   &none &none &none &none &none
